{
	"manifest_version": 2,

	"name": "VB-Paradise Look'n'Feel",
	"description": "Dieses Plugin ver√§ndert die Darstellung von VB-Paradise.",
	"version": "1.1.1",

	"permissions": [
		"storage"
	],

	"content_scripts": [
		{
			"matches": ["http://vb-paradise.de/*",
						"http://*.vb-paradise.de/*",
						"https://vb-paradise.de/*",
						"https://*.vb-paradise.de/*"],
			"js": ["vbp.js"]
		}
	],
	"options_page": "options.htm"
}